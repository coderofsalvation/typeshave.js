{
  "name": "typeshave",
  "version": "0.2.48",
  "description": "Lightweight typesafe function wrapper for PHP/JS. Wrap functions,forms,api's and more with jsonschema. http://typeshave.isvery.ninja",
  "homepage": "http://coderofsalvation.github.io/typeshave",
  "main": "index.js",
  "scripts": {
    "transpile": "/bin/bash --login -c 'babel node_modules/tiny-json-validator -d node_modules/tiny-json-validator-es5'",
    "browser": "/bin/bash --login -c 'coffee -c *.coffee test/*.coffee && browserify -r .:typeshave | uglifyjs -o browser/typeshave.min.js'"
  },
  "keywords": [
    "typesafe",
    "assertion",
    "jsonschema",
    "wrapper"
  ],
  "repository": "http://github.com/coderofsalvation/typeshave.js",
  "author": "Leon van Kammen / Coder of Salvation",
  "license": "BSD",
  "dependencies": {
    "tiny-json-validator": "git+https://github.com/coderofsalvation/tiny-json-validator.git"
  }
}
